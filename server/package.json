{
  "name": "starterapp",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon --watch 'src/**' --ext 'ts,json' --exec 'npx ts-node src/app.ts | npx pino-toke -a 1 :id :method :url :status :res[content-length] :response-time ms | npx pino-pretty -c -t HH:MM:ss -i req,pid,hostname -o \"{req.id} {sql}{msg}\"'",
    "build": "npx tsc",
    "start": "node --enable-source-maps build/app.js",
    "g:migration": "npx knex migrate:make -x ts",
    "test": "npx jest --color --coverage --verbose"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.19",
    "luxon": "^1.26.0",
    "pg": "^8.5.1",
    "pg-promise": "^10.9.4",
    "pino": "^6.11.1",
    "pino-http": "^5.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.26.2",
    "@types/pg": "^7.14.11",
    "@types/pino": "^6.3.6",
    "@types/pino-http": "^5.4.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pino-pretty": "^4.7.0",
    "pino-toke": "^2.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}